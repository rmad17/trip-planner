PORT=3000
DB_URL="host=localhost user= password= dbname= port=5432 sslmode=disable"
SECRET=auth-api-jwt-secret

# LLM Provider Configuration
# Supported providers: gemini, claude, gpt
# Default provider (if not specified, defaults to gemini)
DEFAULT_LLM_PROVIDER=gemini

# API Keys for LLM Providers
# Gemini (Google AI) - https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Claude (Anthropic) - https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GPT (OpenAI) - https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
